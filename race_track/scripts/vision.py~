#!/usr/bin/env python

import rospy
import cv22
import cv_bridge

class img:
  def __init__(self):
#    self.pub = rospy.Publisher("image_topic2", Image)
    self.bridge = CvBridge()
    self.sub = rospy.Subscriber("/front/image_raw/compressed", CompressedImage, self.callback, queue_size = 1)
    
  def callback(self, data):
    cv_image = self.bridge.imgmsg_to_cv2(data, "bgr8")
    
if __name__ == '__main__':
  ic = img()
  rospy.init_node('image_show', anonymous = True)
  try:
    rospy.spin()  
  except KeyboardInterrupt:
    print("Shutting down")
  cv2.destroyAllWindows()
